type Language = C | JAVA | OCAML | RUST | OTHER of (String)

let [Language] -> String hasObjects = lambda (Language lang) match (Language lang) with
|   (C)        -> "nah"
|   (JAVA)     -> "heck yeah"
|   (OCAML)    -> "apparently"
|   (RUST)     -> "no idea"
|   (OTHER("Grouper")) -> "heck no"
|   (OTHER(s)) -> s + "++"
end

(* let x = "OK" in print x *)
let Language js = OTHER ("JavaScript") in print (hasObjects js)